{
  "name": "claims-manager-ang",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host localhost --open --port 4200 --proxy-config proxy.conf.js --base-href=/sample-context/",
    "start-mock": "ng serve --host localhost --port 80 --open --proxy-config mock.proxy.conf.js --base-href=/sample-context/",
    "start-static-ip": "ng serve --host 10.0.0.18 --open --port 4200 --proxy-config proxy.conf.js --base-href=/sample-context/",
    "start-mock-static-ip": "ng serve --host 10.0.0.18 --port 80 --open --proxy-config mock.proxy.conf.js --base-href=/sample-context/",
    "build": "ng build --prod --output-path ../backend/build/ui/static --base-href=/sample-context/",
    "dev": "ng build --output-path ../Myriad-Mobi-Poc-WEB/src/main/resources/static --watch=true --delete-output-path=false --base-href=/sample-context/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "mock": "start npm run start-mock-server && npm run start-mock",
    "start-mock-server": "node mock/mock-server.js",
    "mock-static-ip": "start npm run start-mock-server && npm run start-mock-static-ip"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.8",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "~11.2.8",
    "@angular/compiler": "~11.2.8",
    "@angular/core": "~11.2.8",
    "@angular/elements": "~11.2.8",
    "@angular/forms": "~11.2.8",
    "@angular/localize": "~11.2.8",
    "@angular/material": "^11.2.13",
    "@angular/platform-browser": "~11.2.8",
    "@angular/platform-browser-dynamic": "~11.2.8",
    "@angular/router": "~11.2.8",
    "document-register-element": "^1.7.2",
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "morgan": "^1.10.0",
    "npm": "^6.14.13",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.7",
    "@angular/cli": "^11.2.7",
    "@angular/compiler-cli": "~11.2.8",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
